Cell[BoxData[
 RowBox[{
  RowBox[{"PolTrig", "[", 
   RowBox[{"n_", ",", "x_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"a0", "+", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"ToExpression", "[", 
        RowBox[{"StringJoin", "[", 
