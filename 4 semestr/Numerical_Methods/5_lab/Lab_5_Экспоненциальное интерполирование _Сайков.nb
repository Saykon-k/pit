Cell[BoxData[
 RowBox[{
  RowBox[{"mainF1", "[", 
   RowBox[{"n_", ",", "x_", ",", "y_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"sys", ",", "element", ",", "res", ",", "first"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"element", " ", "=", " ", 
      RowBox[{"Join", "[", 
       RowBox[{
        RowBox[{"{", "a0", "}"}], ",", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"ToExpression", "[", 
           RowBox[{"StringJoin", "[", 
            RowBox[{
             RowBox[{"ToString", "[", "a", "]"}], ",", 
             RowBox[{"ToString", "[", "i", "]"}]}], "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}], ",", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"ToExpression", "[", 
           RowBox[{"StringJoin", "[", 
            RowBox[{
             RowBox[{"ToString", "[", "b", "]"}], ",", 
             RowBox[{"ToString", "[", "i", "]"}]}], "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], 
       "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"sys", "=", " ", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"PolTrig", "[", 
          RowBox[{"n", ",", "#"}], "]"}], "&"}], ",", "x"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"sys", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"PolEx2", "[", 
         RowBox[{
          RowBox[{"sys", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", 
          RowBox[{"y", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "@", "y"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res", " ", "=", " ", 
      RowBox[{"NSolve", "[", 
       RowBox[{"sys", ",", "element"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"first", " ", "=", " ", 
      RowBox[{"res", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1", ",", "2"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res", " ", "=", " ", 
      RowBox[{"Drop", "[", 
       RowBox[{
        RowBox[{"res", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", "1"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"first", "+", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Cosh", "[", 
           RowBox[{"k", "*", "x1"}], "]"}], 
          RowBox[{"res", "[", 
           RowBox[{"[", 
            RowBox[{"k", ",", "2"}], "]"}], "]"}]}], "+", 
         RowBox[{
          RowBox[{"Sinh", "[", 
           RowBox[{"k", "*", "x1"}], "]"}], 
          RowBox[{"res", "[", 
           RowBox[{"[", 
            RowBox[{
             RowBox[{"n", "+", "k"}], ",", "2"}], "]"}], "]"}]}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}]}]}], 
